{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "server/**/*"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.ts"
      }
    ]
  }
  